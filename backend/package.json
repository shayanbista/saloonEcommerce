   "scripts": {
      "start:dev": "nodemon ./src/app.ts/",
      "build": "tsc -p .",
      "start:prod": "node./disp/app.js",
      "typeorm": "typeorm-ts-node-commonjs",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d ./src/datasource.ts",
